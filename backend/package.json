{
  "name": "volcengine-api-proxy",
  "version": "1.0.0",
  "description": "Proxy service for Volcengine API calls",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "prestart": "node scripts/check-dependencies.js",
    "check-deps": "node scripts/check-dependencies.js",
    "install-ffmpeg": "bash scripts/install-ffmpeg.sh",
    "db:init": "node scripts/init-database.js",
    "db:verify": "node scripts/verify-database.js",
    "db:migrate": "node db/migrate.js",
    "db:migrate:status": "node db/migrate.js status",
    "db:test": "node -e \"require('./db/connection').testConnection().then(() => process.exit(0)).catch(() => process.exit(1))\"",
    "test:api": "node scripts/test-apis.js",
    "test:e2e": "node test-e2e.js",
    "test": "jest --runInBand"
  },
  "dependencies": {
    "@volcengine/openapi": "^1.32.0",
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "cos-nodejs-sdk-v5": "^2.11.19",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mysql2": "^3.15.3",
    "node-cron": "^4.2.1",
    "uuid": "^9.0.0",
    "wechatpay-node-v3": "^2.2.1"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "jest": "^30.2.0",
    "nodemon": "^3.0.1",
    "supertest": "^7.2.2"
  }
}